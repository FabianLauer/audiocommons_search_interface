{
	"extends": "tslint:latest",
	"rules": {
		"object-literal-sort-keys": false,
		"indent": [
			true,
			"tabs"
		],
		"forin": false,
		"new-parens": false,
		"jsdoc-format": true,
		"interface-name": [
			true,
			"always-prefix"
		],
		"quotemark": [
			true,
			"single",
			"avoid-escape"
		],
		"semicolon": [
			true,
			"always"
		],
		"variable-name": [
			true,
			"ban-keywords",
			"check-format",
			"allow-leading-underscore"
		],
		"no-var-keyword": false,
		"whitespace": [
			true,
			"check-branch",
			"check-operator"
		],
		"member-access": [
			true,
			"check-accessor"
		],
		"member-ordering": [
			false
		],
		"curly": false,
		"ordered-imports": [
			false
		],
		"no-debugger": true,
		"no-consecutive-blank-lines": [
			false
		],
		"max-line-length": [
			true,
			120
		],
		"trailing-comma": [
			true,
			{
				"multiline": "never",
				"singleline": "never"
			}
		],
		"no-bitwise": false,
		"no-namespace": false,
		"only-arrow-functions": false,
		"no-angle-bracket-type-assertion": false,
		"arrow-parens": false,
		"max-classes-per-file": false,
		"prefer-conditional-expression": false,
		"no-object-literal-type-assertion": false,
		"one-line": false,
		"no-submodule-imports": false
	},
	"rulesDirectory": []
}